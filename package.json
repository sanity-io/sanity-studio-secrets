{
  "name": "sanity-secrets",
  "version": "0.1.0",
  "homepage": "https://sanity.io",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-studio-secrets/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-studio-secrets.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "main": "dist/index.js",
  "module": "dist/sanity-secrets.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@sanity/client": "^3.2.0",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@sanity/ui": "^0.36.12",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "husky": "^5.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "@sanity/ui": "^0.36.12",
    "react": ">=16",
    "styled-components": "^5.3.0"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/sanity-secrets.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sanity-secrets.esm.js",
      "limit": "10 KB"
    }
  ]
}
